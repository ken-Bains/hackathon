{
    "manifest_version": 2,
    "name": "Rate My Netflix",
    "version": "1.0",
    "description": "While browsing Neflix, easily view any show or movie's IMBD ratings",
    "content_scripts": [
        {
          "matches": ["https://www.imbd.com/title*", "https://www.netflix.com/*"],
          "js": ["jquery.min.js", "./main.js"]
        }
      ],
    "css": ["index.css"]
  }